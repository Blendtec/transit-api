{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {
        "no-unused-expression": true
    },
    "rules": {
        "eofline": false,
        "quotemark": [
            true,
            "single"
        ],
        "indent": [true, "spaces"],
        "member-access": [
            false
        ],
        "ordered-imports": [
            false
        ],
        "max-line-length": [
            150
        ],
        "member-ordering": [
            false
        ],
        "curly": [true],
        "interface-name": [
            false
        ],
        "array-type": [
            false
        ],
        "no-empty-interface": false,
        "no-empty": true,
        "arrow-parens": false,
        "object-literal-sort-keys": false,
        "no-unused-expression": true,
        "no-unused-variable": [true, "check-parameters"],
        "max-classes-per-file": [
            false
        ],
        "variable-name": [
            false
        ],
        "one-line": [
            false
        ],
        "one-variable-per-declaration": true
    },
    "rulesDirectory": []
}